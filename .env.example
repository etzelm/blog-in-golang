# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# DynamoDB Tables
ARTICLES=Test-Articles
LISTINGS=Test-Listings

# Google OAuth (for React frontend authentication)
GAPI=your_google_client_id.apps.googleusercontent.com

# CloudFront Distribution IDs (Production only)
DISTRIBUTION_ID1=your_cloudfront_distribution_id
DISTRIBUTION_ID2=your_backup_distribution_id

# Production Deployment Secrets (used in GitHub Actions)
NAS_ADDR=your_nas_server_address
NAS_USER=deployment_user
NAS_PASS=deployment_password
NAS_ARGS="-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null"
NAS_DIR=/path/to/deployment/directory
NAS_BIN=/usr/local/bin

GCP_ADDR=your_gcp_instance_ip
GCP_USER=gcp_deployment_user
GCP_PASS=gcp_deployment_password
GCP_ARGS="-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null"
GCP_DIR=/path/to/gcp/deployment
GCP_BIN=/usr/bin

# GitHub Token (for automated PR management)
TOKEN=your_github_personal_access_token